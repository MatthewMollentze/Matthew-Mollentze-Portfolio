{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://www.matthewmollentze.com/",
    "title": "Matthew Mollentze - Portfolio",
    "titleTemplate": "Matthew Mollentze%s · Portfolio",
    "description": "Matthew Mollentze%s · Portfolio",
    "author": "Matthew Mollentze",
    "avatar": "../content/images/gatsby.png",
    "bio": "This biography will be used in the footer area of an article and can contain some more details about the author.",
    "social": {
      "github": "https://github.com/MatthewMollentze/",
      "medium": "https://medium.com/@matthew.mollentze/",
      "linkedin": "https://www.linkedin.com/in/matthew-mollentze/",
      "mail": "mailto:matthewmollentze@gmail.com"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "Matthew Mollentze" },
    "navigation": {
      "header": [
        { "label": "Skills", "url": "/#skills" },
        { "label": "Projects", "url": "/#projects" },
        { "label": "Designs", "url": "/#designs" }
      ],
      "ctaButton": {
        "openNewTab": true,
        "label": "Resume",
        "url": "/resume.pdf"
      },
      "footer": [
        { "label": "Privacy", "url": "/privacy" },
        { "label": "Imprint", "url": "/imprint" }
      ]
    },
    "featureToggles": {
      "useDarkModeAsDefault": true,
      "useDarkModeBasedOnUsersPreference": true,
      "useCookieBar": false
    }
  }
}
